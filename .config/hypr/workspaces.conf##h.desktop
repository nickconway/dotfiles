bind = $mainMod, B, focusworkspaceoncurrentmonitor, 1
bind = $mainMod, T, focusworkspaceoncurrentmonitor, 2
bind = $mainMod, D, focusworkspaceoncurrentmonitor, 2
bind = $mainMod, M, focusworkspaceoncurrentmonitor, 2
bind = $mainMod, F, focusworkspaceoncurrentmonitor, 3

workspace = 1, on-created-empty: flatpak run com.google.Chrome
workspace = 2, on-created-empty: kitty; flatpak run com.discordapp.Discord; thunderbird-beta
